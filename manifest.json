{
    "manifest_version": 2,
    "name": "TabpagesTransparentTool",
    "version": "0.0.0.1",
    "browser_action": {
        "default_icon": "nihonchizu.png"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "tabs",
        "background",
        "<all_urls>"
    ],
    "icons": {
        "16": "nihonchizu.png"
    },
    "background": {
        "x_page": "background.html",
        "scripts": ["background.js"],
        "persistent": false
    }
}
